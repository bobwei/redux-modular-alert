  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "babel src --out-dir lib",
    "build:watch": "babel src --out-dir lib --watch",
    "precommit": "lint-staged --verbose",
    "format": "prettier --single-quote --trailing-comma all '{src,__{tests,mocks}__}/**/*.{js,jsx}' --write",
    "test": "jest --onlyChanged --watch",
    "test:all": "jest",
    "lint-staged": "lint-staged --verbose",
    "flow": "flow",
    "prepublish": "yarn run clean && yarn run test:all && yarn run build",
    "clean": "rimraf lib",
    "dev": "nodemon src/index.js --exec babel-node"
  },
  "author": "bob61016@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "v5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.51.1",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.3",
    "react-addons-test-utils": "^15.6.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.24.0",
    "redux": "^3.7.0",
    "redux-actions": "^2.0.3",
    "reselect": "^3.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.24.0",
    "redux": "^3.7.0",
    "redux-actions": "^2.0.3",
    "reselect": "^3.0.1"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/env-setup.js",
      "<rootDir>/node_modules/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/env-setup.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn run format",
      "git add"
    ]
  }
}
